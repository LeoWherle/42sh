  echo 'echo $?'
[926-END]

[927]
NAME="triple * test"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cd ..'
  echo 'find */*/*'
[927-END]
